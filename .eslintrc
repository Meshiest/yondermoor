{
  "env": {
    "es6": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "quotes": ["error", "single"],
    "no-var": "warn",
    "semi": ["error", "always"],
    "indent": ["error", 2],
    "comma-dangle": ["error", "always"],
    "eqeqeq": "warn",
  }
}
