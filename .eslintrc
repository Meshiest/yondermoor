{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "React": true,
    "ReactDOM": true
  },
  "ecmaVersion": 6,
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "quotes": ["error", "single"],
    "no-var": "warn",
    "semi": ["error", "always"],
    "indent": ["error", 2],
    "eqeqeq": "warn",
    "no-unused-vars": "warn"
  }
}
